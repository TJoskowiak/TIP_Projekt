<Window x:Name="WindowCallHistory" x:Class="VOiP_Communicator.CallHistoryWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VOiP_Communicator"
        mc:Ignorable="d"
        Title="Call History" Height="450" Width="646.218">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="67*"/>
            <ColumnDefinition Width="207*"/>
            <ColumnDefinition Width="45*"/>
        </Grid.ColumnDefinitions>
        <ListView x:Name="ListView_CallHistory" HorizontalAlignment="Left" Height="341" Margin="10,10,0,0" VerticalAlignment="Top" Width="618" Grid.ColumnSpan="3" SelectionChanged="ListView_CallHistory_SelectionChanged"/>
        <Button x:Name="Button_Call" Content="Call" HorizontalAlignment="Left" Height="35" Margin="10,374,0,0" VerticalAlignment="Top" Width="286" Click="Button_Click" Grid.ColumnSpan="2"/>
        <Button x:Name="Button_Exit" Content="Exit" HorizontalAlignment="Left" Height="35" Margin="208,374,0,0" VerticalAlignment="Top" Width="286" Grid.Column="1" Grid.ColumnSpan="2"/>
    </Grid>
</Window>
